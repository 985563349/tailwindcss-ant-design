{
  "name": "tailwindcss-ant-design",
  "version": "0.0.0",
  "description": "A Tailwind CSS plugin for Ant Design integration",
  "type": "module",
  "scripts": {
    "dev": "tsup --watch & pnpm --filter example dev",
    "test": "vitest",
    "build": "tsup",
    "release": "release-it"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.ts",
        "require": "./dist/index.d.cts"
      },
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/985563349/tailwindcss-ant-design.git"
  },
  "keywords": [
    "tailwindcss-ant-design"
  ],
  "author": "Jee",
  "license": "MIT",
  "homepage": "https://github.com/985563349/tailwindcss-ant-design#readme",
  "packageManager": "pnpm@10.11.0",
  "peerDependencies": {
    "antd": "^5",
    "tailwindcss": ">=3.0.0 || >=4.0.0-alpha.20 || >=4.0.0-beta.1"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^10.0.1",
    "@types/lodash": "^4.17.17",
    "antd": "^5.25.4",
    "release-it": "^19.0.3",
    "tailwindcss": "^4.1.7",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4"
  }
}
